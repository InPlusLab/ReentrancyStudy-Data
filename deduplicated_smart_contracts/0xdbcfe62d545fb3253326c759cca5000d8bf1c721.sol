        TICKET_PRICE = _price;
    }

    function setPrice(uint256 _newPrice) external onlyOwner {
        TICKET_PRICE = _newPrice;
    }

    function setMaxTickets(uint256 _newMax) external onlyOwner {
        MAX_TICKETS = _newMax;
    }

