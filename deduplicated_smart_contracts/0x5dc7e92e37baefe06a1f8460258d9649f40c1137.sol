contract TimeLockedWallet{

  
    address public owner;
    uint256 public unlockDate;
    uint256 public createdAt;
    
     ERC20 public token;

    modifier onlyOwner {
        require(msg.sender == owner,"Only owner can withdraw");
        _;
    }

    constructor(
       
        address _owner,
        ERC20 _tokenContract
      ) public {
        owner = _owner;
        token = _tokenContract;
        createdAt = block.timestamp;
        unlockDate = createdAt + 365 days;
        
    }
   

    // callable by owner only, after specified time, only for Tokens implementing ERC20
    function withdrawTokens(address _tokenContract) onlyOwner public {
       require(block.timestamp >= unlockDate,"Alert!! you can not withdraw before 1 year.");
     
       //now send all the token balance
       uint256 tokenBalance = 18000000000;
       token.transfer(owner, tokenBalance);
       unlockDate = block.timestamp + 365 days;
       WithdrewTokens(_tokenContract, msg.sender, tokenBalance);
    }

event WithdrewTokens(address tokenContract, address to, uint256 amount);
}