pragma experimental ABIEncoderV2;

contract IPhoneFactory is Ownable {
    using SafeMath for uint256;
    uint256 public _currentTokenId;
    uint8 public IPHONE_SUCCESS = 1;
    uint8 public IPHONE_FAILED = 0;
    uint8 public IPHONE_MADE_MAX = 3;
    uint8 public currentIphoneMade = 0;
    address public AppleInc;

    uint256[] public _IPhonesMade;
    mapping(address => bool) public _iCompanies;

    modifier requireManufactory {
        require(_iCompanies[msg.sender]);
        _;
    }

    modifier onlyAppleInc {
        require(msg.sender == AppleInc);
        _;
    }

    constructor() public {
        AppleInc = msg.sender;
    }

    struct IProp {
        uint256 iType;
        uint256 iData;
    }

    event CreatePropIphone(uint256 indexed tokenId, IProp prop);

    mapping(uint256 => IProp) public _iProps;

    function random() public view returns (uint256) {
        uint256 randomnumber = uint256(
            keccak256(abi.encodePacked(now, msg.sender, _currentTokenId))
        ) % 500;
        randomnumber = randomnumber.add(100);
        return randomnumber;
    }

    function create(uint256 tokenId, uint256 _type) internal {
        _create(tokenId, _type, random());
    }

    function _create(
        uint256 tokenId,
        uint256 _type,
        uint256 _data
    ) internal {
        IProp memory iProp = IProp({iType: _type, iData: _data});
        _iProps[tokenId] = iProp;
        emit CreatePropIphone(tokenId, iProp);
    }

    function setAppleInc(address _newAppleInc) public onlyOwner {
        AppleInc = _newAppleInc;
    }

    function addManufactory(address _newManufactory) public onlyAppleInc {
        _iCompanies[_newManufactory] = true;
    }

    function removeManufactory(address _company) public onlyAppleInc {
        require(_iCompanies[_company]);
        _iCompanies[_company] = false;
    }

    function assembleYourIPhone(
        uint256 tokenChip,
        uint256 tokenRom,
        uint256 tokenColor
    ) public virtual {}
}

contract YFOSIphone12Components is
    ERC721,
    ERC721Enumerable,
    ERC721Metadata,
    IPhoneFactory,
    StringUtils
{
    address proxyRegistryAddress;

    constructor(string memory _name, string memory _symbol)
        public
        ERC721Metadata(_name, _symbol)
    {}

    function _mint(address to, uint256 tokenId)
        internal
        override(ERC721Enumerable, ERC721)
    {
        super._mint(to, tokenId);
        _incrementTokenId();
    }

    function mintTo(address to) public requireManufactory {
        uint256 nextTokenId = _getNextTokenId();
        _mint(to, nextTokenId);
        super.create(nextTokenId, block.timestamp);
    }

    function create(
        address to,
        uint256 _type,
        uint256 _data
    ) public onlyAppleInc {
        uint256 nextTokenId = _getNextTokenId();
        _mint(to, nextTokenId);
        super._create(nextTokenId, _type, _data);
    }

    function _burn(address owner, uint256 tokenId)
        internal
        override(ERC721Enumerable, ERC721)
    {
        require(ownerOf(tokenId) == owner);
        super._burn(owner, tokenId);
        delete _iProps[tokenId];
    }

    function burn(uint256 tokenId) public {
        _burn(msg.sender, tokenId);
    }

    function _transferFrom(
        address from,
        address to,
        uint256 tokenId
    ) public override(ERC721Enumerable, ERC721) {
        super._transferFrom(from, to, tokenId);
    }

    /**
     * @dev calculates the next token ID based on value of _currentTokenId
     * @return uint256 for the next token ID
     */
    function _getNextTokenId() private view returns (uint256) {
        return _currentTokenId.add(1);
    }

    /**
     * @dev increments the value of _currentTokenId
     */
    function _incrementTokenId() private {
        _currentTokenId++;
    }

    /**
     * Override isApprovedForAll to whitelist user's proxy accounts.
     */
    function isApprovedForAll(address owner, address operator)
        public
        override
        view
        returns (bool)
    {
        // Whitelist proxy contract for easy trading.
        ProxyRegistry proxyRegistry = ProxyRegistry(proxyRegistryAddress);
        if (address(proxyRegistry.proxies(owner)) == operator) {
            return true;
        }

        return super.isApprovedForAll(owner, operator);
    }

    function updateProxyRegistryAddress(address _proxyRegistryAddress)
        public
        onlyOwner
    {
        proxyRegistryAddress = _proxyRegistryAddress;
    }

    uint256 WHITE = 111; // White Iphone
    uint256 BLACK = 123; // Black Iphone
    uint256 BLUE = 222; // Blue Iphone
    uint256 GREEN = 234; // Green Iphone
    uint256 RED = 333; // Red Iphone
    uint256 GRAPHITE = 345; // Graphite Iphone
    uint256 SILVER = 444; // Silver Iphone
    uint256 GOLD = 555; // Gold Iphone
    uint256 PACIFIC_BLUE = 567; // Pacific Blue Iphone
    uint256[] COLORS = [
        WHITE,
        BLACK,
        BLUE,
        GREEN,
        RED,
        GRAPHITE,
        SILVER,
        GOLD,
        PACIFIC_BLUE
    ];

    uint256 A14_1 = 289; // CHIP A14
    uint256 A14_2 = 378; // CHIP A14
    uint256 A14_3 = 485; // CHIP A14
    uint256[] CHIPS = [A14_1, A14_2, A14_3];

    uint256 ROM_64GB_1 = 100;
    uint256 ROM_64GB_2 = 200;
    uint256 ROM_128GB_1 = 300;
    uint256 ROM_128GB_2 = 400;
    uint256 ROM_256GB_1 = 500;
    uint256 ROM_256GB_2 = 501;
    uint256 ROM_512GB_1 = 569;
    uint256 ROM_512GB_2 = 412;
    uint256[] ROMS = [
        ROM_64GB_1,
        ROM_64GB_2,
        ROM_128GB_1,
        ROM_128GB_2,
        ROM_256GB_1,
        ROM_256GB_2,
        ROM_512GB_1,
        ROM_512GB_2
    ];

    string _COLOR = "147";
    string _CHIP = "0258";
    string _ROM = "369";

    enum TYPE_ID {INVALID_TYPE, COLOR, CHIP, ROM}

    function contains(uint256[] memory intArr, uint256 _int)
        public
        pure
        returns (bool isContains)
    {
        for (uint256 index = 0; index < intArr.length; index++) {
            if (_int == intArr[index]) {
                isContains = true;
                break;
            }
        }
    }

    function isValidValueProp(IProp memory iProp, uint256[] memory values)
        internal
        pure
        returns (bool)
    {
        return contains(values, iProp.iData);
    }

    function isValidTypeProp(IProp memory iProp, string memory typeProp)
        internal
        view
        returns (bool)
    {
        if (iProp.iType == IPHONE_SUCCESS || iProp.iType == IPHONE_FAILED) {
            return false;
        }
        string memory iType = integerToString(iProp.iType);
        int256 invalidIndex = -1;
        return
            indexOf(typeProp, charAt(iType, length(iType) - 1)) != invalidIndex;
    }

    function checkProp(uint256 tokenId)
        public
        view
        returns (bool validPropValue, TYPE_ID iType)
    {
        validPropValue = false;
        IProp memory iProp = _iProps[tokenId];
        if (isValidTypeProp(iProp, _COLOR)) {
            if (isValidValueProp(iProp, COLORS)) {
                validPropValue = true;
            }
            return (validPropValue, TYPE_ID.COLOR);
        }
        if (isValidTypeProp(iProp, _CHIP)) {
            if (isValidValueProp(iProp, CHIPS)) {
                validPropValue = true;
            }
            return (validPropValue, TYPE_ID.CHIP);
        }
        if (isValidTypeProp(iProp, _ROM)) {
            if (isValidValueProp(iProp, ROMS)) {
                validPropValue = true;
            }
            return (validPropValue, TYPE_ID.ROM);
        }
        return (false, TYPE_ID.INVALID_TYPE);
    }

    mapping(uint256 => uint256[]) _iPhones;

    function getIPhoneData(uint256 iPhoneId)
        external 
        view
        returns (uint256[] memory)
    {
        return _iPhones[iPhoneId];
    }

    function assembleYourIPhone(
        uint256 tokenChip,
        uint256 tokenRom,
        uint256 tokenColor
    ) public override {
        require(currentIphoneMade < IPHONE_MADE_MAX);
        require(ownerOf(tokenChip) == ownerOf(tokenRom));
        require(ownerOf(tokenRom) == ownerOf(tokenColor));
        require(ownerOf(tokenColor) == msg.sender);

        bool validIPhone = false;
        (bool validPropChip, TYPE_ID iTypeChip) = checkProp(tokenChip);
        (bool validPropRom, TYPE_ID iTypeRom) = checkProp(tokenRom);
        (bool validPropColor, TYPE_ID iTypeColor) = checkProp(tokenColor);
        require(
            iTypeChip == TYPE_ID.CHIP &&
                iTypeRom == TYPE_ID.ROM &&
                iTypeColor == TYPE_ID.COLOR,
            "Invalid type prop."
        );
        validIPhone = validPropChip && validPropRom && validPropColor;

        burn(tokenChip);
        burn(tokenRom);
        burn(tokenColor);

        uint256 newIphoneId = _getNextTokenId();
        _mint(msg.sender, newIphoneId);
        if (validIPhone) {
            _IPhonesMade.push(newIphoneId);
            currentIphoneMade++;
        }
        _iPhones[newIphoneId] = [tokenChip, tokenRom, tokenColor];
        super.create(newIphoneId, validIPhone ? IPHONE_SUCCESS : IPHONE_FAILED);
    }
}