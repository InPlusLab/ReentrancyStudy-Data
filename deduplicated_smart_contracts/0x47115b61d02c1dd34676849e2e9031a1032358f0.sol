            (, answer, , updatedAt, ) = getBtcRoundData(roundId);
            isFuture = getBtcLastRound() < roundId;
            parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 400 400" id="g"><rect width="100%" height="100%" fill="black" /><g stroke="none" stroke-width="1" fill="none" fill-rule="evenodd" transform="translate(116,50) scale(4.2)"><g id="ibtc" transform="translate(-220.000000, -310.000000)" fill="#F7931A"><g transform="translate(220.000000, 310.000000)"><path d="M20,0 C31.045695,0 40,8.954305 40,20 C40,31.045695 31.045695,40 20,40 C8.954305,40 0,31.045695 0,20 C0,8.954305 8.954305,0 20,0 Z M19.5154543,6.33976715 L18.5185467,10.060277 C17.6758378,9.86154045 16.7055446,9.58834429 13.9565191,8.83788541 L13.3077489,11.2591287 C15.1113671,11.70836 16.084151,11.845384 15.9915373,13.0542334 L13.2619144,23.2413248 C12.8801666,24.1178529 12.1845538,23.7905067 10.565028,23.3257233 L9.38342877,25.904891 C13.5290313,27.0157018 13.9415486,27.1420536 13.9415486,27.1420536 L13.0878078,30.3282576 L15.3614657,30.9374824 L16.2033429,27.7955541 C16.8194343,27.9764534 17.4247686,28.1386522 18.0005857,28.292942 L17.1626631,31.4201118 L19.436321,32.0293366 L20.2900618,28.8431326 C24.1545115,29.6570543 26.9579838,29.379711 28.2588749,25.8830669 C29.2959739,23.0753627 28.2818561,21.4268991 26.2648067,20.3326463 C27.7420434,20.0164028 28.8853379,19.0567934 29.2195251,17.0417712 C29.688479,14.2874689 27.7230411,12.7955726 24.8670599,11.761356 L25.8639675,8.04084614 L23.5903095,7.43162133 L22.6210962,11.0487746 C22.0305205,10.8905303 21.4212317,10.74309 20.8119898,10.5956623 L21.7891122,6.94899196 L19.5154543,6.33976715 Z M18.3205371,20.0711804 C19.8559871,20.482603 24.6725885,21.3143304 23.8893013,24.2375982 C23.1337209,27.0574625 18.5188762,25.4727686 16.9834262,25.061346 L18.3205371,20.0711804 Z M20.1442444,13.265012 C21.4287056,13.6091823 25.5512775,14.2866316 24.835255,16.9588637 C24.1508689,19.5130276 20.2142699,18.1417054 18.9297619,17.7975225 L20.1442444,13.265012 Z"></path></g></g></g><text x="50%" y="280" text-anchor="middle" style="font-size:42px;fill:white;">';
        }
        else if(tokenId <= 30000){//ETH price 
            uint80 roundId = tokenId2RoundId(tokenId);
            (, answer, , updatedAt, ) = getEthRoundData(roundId);
            isFuture = getEthLastRound() < roundId;
            parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 400 400" id="g"><rect width="100%" height="100%" fill="#222222" /><g transform="translate(116,50) scale(0.4)"><path fill="#b7f9f6" d="M208 0l-3 10v275l3 3 128-76z"/><path fill="#f0cdc2" d="M208 0L80 212l128 76V154z"/><path fill="#ccb6f6" d="M208 312l-2 2v98l2 5 128-180z"/><path fill="#f0cdc2" d="M208 417V312L80 237z"/><path fill="#c9b2f5" d="M208 288l128-76-128-58z"/><path fill="#8baef3" d="M80 212l128 76V154z"/></g><text x="50%" y="280" text-anchor="middle" style="font-size:42px;fill:white;">';
        }
        else{
            return string(abi.encodePacked('data:application/json;base64,', Base64.encode(bytes('{"name": "CryptoPrices # N/A", "description": "tokenId error"}'))));
        }
        
        (uint year, uint month, uint day, uint hour, uint minute, uint second) = BokkyPooBahsDateTimeLibrary.timestampToDateTime(uint(updatedAt));
        
        if (isFuture){
            parts[1] = ' N/A';
        }
        else{
            parts[1] = Strings.toString(uint256(answer),8);
        }
        parts[2] = '<tspan style="font-size:26px;fill:white;"> USD</tspan></text><text x="50%" y="320" text-anchor="middle" style="font-size: 20px;fill:white;">';
        if (isFuture){
            parts[8] = 'This is a future NFT';
        }
        else{
            parts[3] = Strings.toString(year);
            parts[4] = '-';
            parts[5] = Strings.to2String(month);
            parts[6] = '-';
            parts[7] = Strings.to2String(day);
            parts[8] = ' ';
            parts[9] = Strings.to2String(hour);
            parts[10] = ':';
            parts[11] = Strings.to2String(minute);
            parts[12] = ':';
            parts[13] = Strings.to2String(second);
        }
        parts[14] = '</text></svg>';

        string memory output = string(abi.encodePacked(parts[0],parts[1],parts[2],parts[3],parts[4],parts[5],parts[6],parts[7]));
        output = string(abi.encodePacked(output, parts[8], parts[9],parts[10],parts[11],parts[12],parts[13],parts[14]));
        
        string memory json = Base64.encode(bytes(string(abi.encodePacked('{"name": "CryptoPrices #', Strings.toString(tokenId), '", "description": "Crypto Prices memorizes crypto asset prices in the timeline. Every NFT presents a past or even a future price of cryptocurrency that everyone can collect and commemorate.", "image": "data:image/svg+xml;base64,', Base64.encode(bytes(output)), '"}'))));
        output = string(abi.encodePacked('data:application/json;base64,', json));

        return output;
    }

    function setBtcPhaseRound(uint16 phaseId, uint64 endRoundId) public onlyOwner {
        while(btcPhaseRounds.length < phaseId){
            btcPhaseRounds.push(0);
        }
        btcPhaseRounds[phaseId - 1] = endRoundId;
    }
    
    function setEthPhaseRound(uint16 phaseId, uint64 endRoundId) public onlyOwner {
        while(ethPhaseRounds.length < phaseId){
            ethPhaseRounds.push(0);
        }
        ethPhaseRounds[phaseId - 1] = endRoundId;
    }
    
    function tokenId2RoundId(uint256 tokenId) internal view returns(uint80){
        uint64 roundCount = 0;
        uint64 roundId = 0;
        uint16 phaseId = 1;
        
        if (tokenId <= 13000){
            roundId = uint64(tokenId);
            uint256 phaseCount = btcPhaseRounds.length;
            for(uint16 i = 0; i < phaseCount; i++){
                phaseId = i + 1;
                roundCount = roundCount + btcPhaseRounds[i];
                if(roundCount >= tokenId){
                    break;
                }
                roundId = roundId - btcPhaseRounds[i];
            }
        }
        else{
            roundId = uint64(tokenId - 13000);
            uint256 phaseCount = ethPhaseRounds.length;
            for(uint16 i = 0; i < phaseCount; i++){
                phaseId = i + 1;
                roundCount = roundCount + ethPhaseRounds[i];
                if(roundCount >= tokenId - 13000){
                    break;
                }
                roundId = roundId - ethPhaseRounds[i];
            }
        }
        return uint80(uint256(phaseId) << PHASE_OFFSET | roundId);
    }
    
    function claim(uint256 tokenId) public payable nonReentrant {
        require(!paused,"Currently paused");  
        require(msg.value >= 10000000000000000, "Must send 0.01 ETH to claim!");
        require(totalSupply() <= 10000, "Total supply is max");
        require(tokenId > 0 && tokenId <= 30000, "Token ID invalid");
        payable(owner()).transfer(msg.value);
        _safeMint(msg.sender, tokenId);
    }
    
    function pause() public onlyOwner {
        paused = true;
    }
    
    function resume() public onlyOwner {
        paused = false;
    }
    
    constructor() ERC721("CryptoPrices", "CPP") Ownable() {}
}

