contract CryptoPrices is ERC721Enumerable, ReentrancyGuard, Ownable {
    
    uint64 [] public btcPhaseRounds = [0, 0, 0, 3708, 9292];
    uint64 [] public ethPhaseRounds = [0, 0, 0, 5056, 11944];
    bool public paused = false;
    
    //constant values
