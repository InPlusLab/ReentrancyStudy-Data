    ////////////////////////////////////////////////////////////
    // CLAIM BY SIGNATURES WITH MINTING
    ////////////////////////////////////////////////////////////
    address private _custodian;
    uint256 public _totalClaimed;
    uint256 public _claimLimit;

    mapping(address => bool) claimPerformed;

    function claimNFT(bytes memory sig) public {
        claimNFTToAddress(msg.sender, sig);
    }

    function claimNFTToAddress(address receiver, bytes memory sig) public {
        require(claimPerformed[receiver] == false, "already claimed");
        require(_totalClaimed < _claimLimit, "claim limit reached");

        bytes32 message = prefixed(keccak256(abi.encodePacked(receiver)));
        address signer = recoverSigner(message, sig);
        require(signer == _custodian, "wrong signature");

        _mint(receiver, _tokenIdTracker.current());
        
        _tokenIdTracker.increment();
        claimPerformed[receiver] = true;
        _totalClaimed = _totalClaimed + 1;
    }

    function resetClaimed(address receiver) public {
        require(hasRole(DEFAULT_ADMIN_ROLE, msg.sender), "admin only");
        claimPerformed[receiver] = false;
    }

    function prefixed(bytes32 addrhash) internal pure returns (bytes32) {
        return keccak256(abi.encodePacked("CLAIM NFT SWS ", addrhash));
    }

    function splitSignature(bytes memory sig)
        internal
        pure
        returns (bytes32 r, bytes32 s, uint8 v)
    {
        require(sig.length == 65, "invalid signature length");

        assembly {
            // first 32 bytes, after the length prefix
            r := mload(add(sig, 32))
            // second 32 bytes
            s := mload(add(sig, 64))
            // final byte (first byte of the next 32 bytes)
            v := byte(0, mload(add(sig, 96)))
        }
    }

    function recoverSigner(bytes32 message, bytes memory sig)
        internal
        pure
        returns (address)
    {
        uint8 v;
        bytes32 r;
        bytes32 s;

        (r, s, v) = splitSignature(sig);

        // Version of signature should be 27 or 28, but 0 and 1 are also possible versions
        if (v < 27) {
            v += 27;
        }

        return ecrecover(message, v, r, s);
    }

