    using BasisPoints for uint256;
    using SafeMath for uint256;

    uint256 public releaseInterval;
    uint256 public releaseStart;
    uint256 public releaseBP;

    uint256 public startingLid;
    uint256 public claimedLid;

    ILidCertifiableToken private lidToken;

    address internal daoWallet;

    modifier onlyAfterStart {
        require(
            releaseStart != 0 && now > releaseStart,
            "Has not yet started."
        );
        _;
    }

    function initialize(
        uint256 _releaseInterval,
        uint256 _releaseBP,
        address owner,
        ILidCertifiableToken _lidToken
