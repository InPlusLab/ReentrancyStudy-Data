  {
    return NFTokenEnumerable._getOwnerNFTCount(_owner);
  }

  /**
   * set wish change price internally
   */
  function _setSignatureChangePrice(
    uint256 _tokenId,
    uint256 _price
  )
    internal
  {
    signatureChangePrice[_tokenId] = _price;
  }

