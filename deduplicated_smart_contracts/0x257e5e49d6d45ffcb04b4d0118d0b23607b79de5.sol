            endICOStage4 = startICOStage4.add(periodICOStage4);

            startICOStage5 = endICOStage4;

		  } 		  

		  _flag_ex = 1;

		}          

	  }

    } 

    if (now >= startICOStage5 && now < endICOStage5 && _flag_ex == 0){

