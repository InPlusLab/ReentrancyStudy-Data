pragma solidity 0.6.6;

contract Intermediate is Withdrawable {
    using SafeERC20 for IERC20;
    using EnumerableSet for EnumerableSet.AddressSet; 

    IERC20 internal constant ETH_TOKEN_ADDRESS = IERC20(
        0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE
    );
    mapping(IERC20 => EnumerableSet.AddressSet) internal approvedWithdrawAddresses;

    IWeth public weth;
    address public wallet;

    event WithdrawFunds(IERC20 indexed token, uint256 amount, address indexed destination);
    event WithdrawAddressApproved(IERC20 token, address addr);
    event WithdrawAddressRevoked(IERC20 token, address addr);

    constructor(address _admin, IWeth _weth, address _wallet) public Withdrawable(_admin) {
        weth = _weth;
        wallet = _wallet;
    }

    receive() external payable {}

    function isAllowWithdraw(IERC20 token, address destination) public view returns(bool) {
        return approvedWithdrawAddresses[token].contains(destination);
    }

    function withdraw(
        IERC20 token,
        uint256 amount,
        address destination
    ) external onlyOperator {
        require(isAllowWithdraw(token, destination),"destination is not approved");
        if (token == ETH_TOKEN_ADDRESS) {
            IERC20(weth).safeTransferFrom(wallet, address(this), amount);
            weth.withdraw(amount);
            (bool success, ) = destination.call{value: amount}("");
            require(success, "withdraw eth failed");
        } else {
            token.safeTransferFrom(wallet, destination, amount);
        }
        emit WithdrawFunds(token, amount, destination);
    }
    
    function convertAndSendWEth(uint _amount)  external onlyOperator {
        require(wallet != address(0), "wallet not set");
        weth.deposit{value: _amount}();
        IERC20(weth).safeTransfer(wallet, _amount);
    }

    function approveWithdrawAddress(
        IERC20 token,
        address addr
    ) external onlyAdmin {
        approvedWithdrawAddresses[token].add(addr);
        emit WithdrawAddressApproved(token, addr);
    }

    function revokeWithdrawAddress(
        IERC20 token,
        address addr
    ) external onlyAdmin {
        approvedWithdrawAddresses[token].remove(addr);
        emit WithdrawAddressRevoked(token, addr);
    }

    function setWallet(address _wallet) external onlyAdmin{
        wallet = _wallet;
    }
}