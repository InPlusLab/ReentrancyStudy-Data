// File: contracts/KasumiNFT.sol

contract KasumiNFT is ERC721, Ownable {
    string public constant name = "kasuminft";
    string public constant symbol = "KNFT";

    struct KNFT {
        string name;
        string json;
        string img;
    }

    uint128 MAX_ITEMS = 10;
    // array of items
    KNFT[] knfts;

    function implementsERC721() public pure returns (bool) {
        return true;
    }

    function baseTokenURI() internal pure returns (string memory) {
        return "ipfs://";
    }

    function tokenURI(uint256 _tokenId) public view returns (string memory) {
        KNFT memory _knft = knfts[_tokenId - 1];
        return Strings.strConcat(
            baseTokenURI(),
            _knft.json
        );
    }

    function totalSupply() public view returns (uint) {
        return knfts.length;
    }

    function mintNFT( string calldata _name, string calldata _json, string calldata _img ) external onlyOwner returns (uint) {
        require(MAX_ITEMS > totalSupply());

        KNFT memory _knft = KNFT({
            name: _name,
            json: _json,
            img: _img
        });
        uint mintId = knfts.push(_knft);

        _mint(owner(), mintId);

        return mintId;
    }
}