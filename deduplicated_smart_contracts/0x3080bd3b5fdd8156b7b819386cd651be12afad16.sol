// File contracts/ethereum/MoonieIDOPayments.sol


pragma solidity 0.8.4;




contract MoonieIDOPayments is Ownable {

    event PaymentAccepted(address indexed sender);


    IERC20 private _token;

    address payable private _bank;

    uint256 private _tokenPrice;

    address[] private _beneficients;

    mapping(address => uint256) public maxEthByAddress;

    mapping(address => uint256) public tokensToDistributeByAddress;

    mapping(address => bool) public ethReceivedByAddress;

    mapping(address => bool) public tokensDistributedByAddress;
    

    constructor(address token_, address payable bank_) Ownable() {
        _tokenPrice = 641025641000;
        _token = IERC20(token_);
        _bank = bank_;
    }

    receive() external payable {
        require(ethReceivedByAddress[msg.sender] == false, "MoonieIDOPayments: ETH already received");
        require(msg.value <= maxEthByAddress[msg.sender], "MoonieIDOPayments: Wrong payment value - too much ETH");
        require(msg.value >= _tokenPrice, "MoonieIDOPayments: Wrong payment value - not enough ETH");
        tokensToDistributeByAddress[msg.sender] = (msg.value / _tokenPrice) * 1000000000000000000;
        require(_bank.send(msg.value), "MoonieIDOPayments: Something went wrong with ETH transfer");
        ethReceivedByAddress[msg.sender] = true;
        emit PaymentAccepted(msg.sender);
    }

    function distributeAll() external onlyOwner {
        uint256 length = _beneficients.length;
        for(uint256 i = 0; i < length; i++) {
            address beneficient = _beneficients[i];
            if (tokensDistributedByAddress[beneficient] == true) continue;
            require(_token.transfer(beneficient, tokensToDistributeByAddress[beneficient]), "MoonieIDOPayments: Something went wrong with MNY transfer");
            tokensDistributedByAddress[beneficient] = true;
        }
    }

    function distributeOne(address beneficient) external onlyOwner {
        require(tokensDistributedByAddress[beneficient] == false, "MoonieIDOPayments: Tokens already distributed");
        require(_token.transfer(beneficient, tokensToDistributeByAddress[beneficient]), "MoonieIDOPayments: Something went wrong with MNY transfer");
        tokensDistributedByAddress[beneficient] = true;
    }

    function addBeneficients(address[] memory beneficients, uint256[] memory amounts) external onlyOwner {
        uint256 length = beneficients.length;
        require(length == amounts.length, "MoonieIDOPayments: Must pass same number of addresses and amounts");
        for(uint256 i = 0; i < length; i++) {
            _beneficients.push(beneficients[i]);
            maxEthByAddress[beneficients[i]] = amounts[i];
        }
    }

    function setBank(address payable bank) external onlyOwner {
        _bank = bank;
    }

    function setMaxEthForAddress(address beneficient, uint256 amount) external onlyOwner {
        maxEthByAddress[beneficient] = amount;
    }

    function withdrawEther() external onlyOwner {
        _bank.transfer(address(this).balance);
    }

    function withdrawERC20(address token, address recipent) external onlyOwner {
        require(IERC20(token).transfer(recipent, IERC20(token).balanceOf(address(this))), "MoonieIDOPayments: Something went wrong with ERC20 withdrawal");
    }
}