contract Withdrawable is Ownable {
    using SafeERC20 for ERC20;
    address constant ETHER = 0xEeeeeEeeeEeEeeEeEeEeeEEEeeeeEeeeeeeeEEeE;

    event LogWithdraw(address indexed _from, address indexed _assetAddress, uint256 amount);

    /**
     * @dev Withdraw asset.
     * @param _assetAddress Asset to be withdrawn.
     */
    function withdraw(address _assetAddress) public onlyOwner {
        uint256 assetBalance;
        if (_assetAddress == ETHER) {
            address self = address(this); // workaround for a possible solidity bug
            assetBalance = self.balance;
            msg.sender.transfer(assetBalance);
        } else {
            assetBalance = ERC20(_assetAddress).balanceOf(address(this));
            ERC20(_assetAddress).safeTransfer(msg.sender, assetBalance);
        }
        emit LogWithdraw(msg.sender, _assetAddress, assetBalance);
    }
}

contract TPArb is Withdrawable {
    uint constant PT_US = 1;
    uint constant PT_BAL = 2;
    uint constant MIN_PROFIT = 5 * 10 ** 18;

    struct Route {
    	address pool1;
	uint pool1_type;

	address pool2;
	uint pool2_type;
    }
    Route[] private routes;

    address constant daiToken = address(0x6B175474E89094C44Da98b954EedeAC495271d0F);
    address constant plexToken = address(0x89b413994D546Cf81b6B36dF294753E15fcd8F35);
    address constant uniswapRouterAddr = address(0x7a250d5630B4cF539739dF2C5dAcb4c659F2488D);
    address constant bpDaiTribePlex = address(0xB7a8c198E5e91504aBdEbf6B8339B8a315E741e3);
    address constant bpBigPlex = address(0xACF33cb6d3552d64Cb664304722014863b643c20);
    address constant bpPlexMix = address(0xDB18e83A45674880447aA3A96a24b134d0cA655f);

    IUniswapV2Router02 uniswapRouter = IUniswapV2Router02(uniswapRouterAddr);
    event Arb(address indexed sender, address swapToken, address startToken, uint startAmt, uint route, uint swap1Bal, uint startBal, uint endBal);

    constructor() public {
        _addRoutes();
    }

    function arb(address swap_token, address start_token, uint start_amount, uint route) external {
        _arb(swap_token, start_token, start_amount, route);
    }

    function bestRoute(address swap_token, address start_token, uint start_amount) external view returns (uint) {
        return _bestRoute(swap_token, start_token, start_amount);
    }

    ////////////////////////////////////////////////////

    function _addRoutes() internal {
        routes.push(Route({
	    pool1: uniswapRouterAddr,
	    pool1_type: PT_US,
	    pool2: bpDaiTribePlex,
	    pool2_type: PT_BAL
	}));
        routes.push(Route({
	    pool2: uniswapRouterAddr,
	    pool2_type: PT_US,
	    pool1: bpDaiTribePlex,
	    pool1_type: PT_BAL
	}));

        routes.push(Route({
	    pool1: uniswapRouterAddr,
	    pool1_type: PT_US,
	    pool2: bpBigPlex,
	    pool2_type: PT_BAL
	}));
        routes.push(Route({
	    pool2: uniswapRouterAddr,
	    pool2_type: PT_US,
	    pool1: bpBigPlex,
	    pool1_type: PT_BAL
	}));

        routes.push(Route({
	    pool1: uniswapRouterAddr,
	    pool1_type: PT_US,
	    pool2: bpPlexMix,
	    pool2_type: PT_BAL
	}));
        routes.push(Route({
	    pool2: uniswapRouterAddr,
	    pool2_type: PT_US,
	    pool1: bpPlexMix,
	    pool1_type: PT_BAL
	}));
    }

    function _estimateBalancer(address sellToken, address buyToken, uint sellAmt, address pool) internal view returns (uint) {
        BPool bpool = BPool(pool);
	uint spotPrice = bpool.getSpotPrice(sellToken, buyToken);
	return sellAmt * spotPrice;
    }

    function _estimateUniswap(address sellToken, address buyToken, uint sellAmt) internal view returns (uint) {
        address[] memory path = new address[](2);
	path[0] = sellToken;
	path[1] = buyToken;
	uint256[] memory amounts = uniswapRouter.getAmountsOut(sellAmt, path);
	return amounts[1];
    }

    function _anyEstimate(address sellToken, address buyToken, uint sellAmt,
    		          address pool, uint pool_type) internal view returns (uint) {
        if (pool_type == PT_US) {
	    return _estimateUniswap(sellToken, buyToken, sellAmt);
	} else if (pool_type == PT_BAL) {
	    return _estimateBalancer(sellToken, buyToken, sellAmt, pool);
	}
    }

    function _bestRoute(address swap_token, address start_token, uint start_amount) internal view returns (uint) {
        uint resRoute = 0;
	uint bestOut = 0;

	for (uint route = 0; route < routes.length; route++) {
	    uint bal_start = start_amount;
	    uint bal_mid = _anyEstimate(start_token, swap_token, bal_start,
	    			    routes[route].pool1,
				    routes[route].pool1_type);
	    uint bal_end = _anyEstimate(swap_token, start_token, bal_mid,
	    			    routes[route].pool1,
				    routes[route].pool1_type);

	    if (bal_end > bestOut) {
	        resRoute = route;
		bestOut = bal_end;
	    }
	}

	return resRoute;
    }

    function _swapBalancer(address sellToken, address buyToken, uint sellAmt, address pool) internal returns (uint amtOut) {
        BPool bpool = BPool(pool);
	IERC20(sellToken).approve(pool, sellAmt);
	(amtOut,) = bpool.swapExactAmountIn(sellToken, sellAmt, buyToken, 1, uint(-1));
    }

    function _swapUniswap(address sellToken, address buyToken, uint sellAmt) internal returns (uint) {
	IERC20(sellToken).approve(uniswapRouterAddr, sellAmt);
        uint deadline = block.timestamp + 30;
	address[] memory path = new address[](2);
	path[0] = sellToken;
	path[1] = buyToken;
	uint256[] memory amts = uniswapRouter.swapExactTokensForTokens(sellAmt, 1, path, address(this), deadline);
	require(amts.length > 0);
	return amts[0];
    }

    function _anySwap(address sellToken, address buyToken, uint sellAmt,
    		      address pool, uint pool_type) internal returns (uint) {
        if (pool_type == PT_US) {
	    return _swapUniswap(sellToken, buyToken, sellAmt);
	} else if (pool_type == PT_BAL) {
	    return _swapBalancer(sellToken, buyToken, sellAmt, pool);
	}
    }

    function _arb(address swapToken, address startToken, uint startAmt, uint route) internal {
	require(startAmt > 0, "startAmt is zero");
	require(route < routes.length, "route must be valid");

	uint startBal = IERC20(startToken).balanceOf(address(this));
	require(startBal >= startAmt, "Starting balance lower than requested amount");

	uint swap1Bal = _anySwap(startToken, swapToken, startAmt,
				 routes[route].pool1,
				 routes[route].pool1_type);
	/* uint swap2Bal = */ _anySwap(swapToken, startToken, swap1Bal,
				 routes[route].pool2,
				 routes[route].pool2_type);

	uint endBal = IERC20(startToken).balanceOf(address(this));
        
	require(endBal >= (startBal + MIN_PROFIT), "ARB FAIL: end < start");

	emit Arb(msg.sender, swapToken, startToken, startAmt, route, swap1Bal, startBal, endBal);
    }
}