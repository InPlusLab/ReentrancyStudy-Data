interface ILGE {
    function depositors(uint256 index) external view returns (address);

    function _accountBalances(address _address) external view returns (uint256);

    function endTime() external view returns (uint256);
}

contract LGEclaim is Ownable {
    using SafeMath for uint256;

    address public lgeAddress;
    ILGE public LGE;
    IERC20 public LPTOKEN;
    IERC20 public YFFS;
    uint256 public totalContributions;
    mapping(address => bool) public claimedAddresses;
    bool public isInit = false;

    modifier requireInit {
        require(isInit, "Run init first.");
        _;
    }

    constructor(
        address _yffs,
        address _lge
    ) public {
        YFFS = IERC20(_yffs);
        lgeAddress = _lge;
        LGE = ILGE(_lge);
    }

    function setLpToken(address _lptoken) external onlyOwner {
        LPTOKEN = IERC20(_lptoken);
    }

    function init() public onlyOwner {
        uint256 endTime = LGE.endTime();
        require(block.timestamp > endTime);
        totalContributions = YFFS.balanceOf(lgeAddress);
        isInit = true;
    }

    function calculateReward(address _checkAddress)
        public
        view
        requireInit
        returns (
            uint256 contribution,
            uint256 contributionIn,
            uint256 reward
        )
    {
        contribution = LGE._accountBalances(_checkAddress);
        contributionIn = contribution.mul(10000).div(totalContributions);
        reward = supplyLPtoken.div(10000).mul(contributionIn);
    }

    function claimReward() public requireInit returns (uint256) {
        require(claimedAddresses[msg.sender] == false);
        uint256 contribution = LGE._accountBalances(msg.sender);
        require(contribution > 0);
        require(isDeposit);
        claimedAddresses[msg.sender] = true;
        (, , uint256 reward) = calculateReward(msg.sender);
        LPTOKEN.transfer(msg.sender, reward);
        return contribution;
    }

    function safeStop() external onlyOwner {
        LPTOKEN.transfer(address(this), LPTOKEN.balanceOf(address(this)));
        supplyLPtoken = 0;
        isDeposit = false;
    }

    uint256 public supplyLPtoken;
    bool public isDeposit = false;

    function depositLPtoken(uint256 amount) public onlyOwner requireInit {
        require(!isDeposit);
        require(amount > 0);
        LPTOKEN.transferFrom(msg.sender, address(this), amount);
        supplyLPtoken = amount;
        isDeposit = true;
    }
}