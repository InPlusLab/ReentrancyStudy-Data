   */
  function _burn(uint256 tokenId) internal whenNotPaused {
    require(_exists(tokenId), "ERC721: token not minted yet");
    address owner = ownerOf(tokenId);
    require(_balances[owner] > 0); // Avoid overflow

    // Clear approvals
    _approve(address(0), tokenId);

