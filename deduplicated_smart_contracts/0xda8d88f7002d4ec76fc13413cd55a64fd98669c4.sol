contract YNGMI is ERC721, Ownable {    

    uint256 public freeMints = 100;
    uint256 public totalMints = 6969;
    uint256 public nextTokenId = 0;

    constructor() ERC721("YNGMI", "YNGMI") {}

    function tokenURI(uint256 tokenId) public view virtual override returns (string memory) {
        require(_exists(tokenId), "bad token");

        return "ipfs://QmY9q8FXW9pYxXheX6uSWGappPgjYYeanqWSttpgZsBgPX";
    }

    function gift(address receiver, uint256 amount) external payable {
        require(amount <= 20, "Over max transaction limit");
        require(nextTokenId + amount <= totalMints, "Over mint limit");
        uint256 value = msg.value;
        uint256 cost = amount * 0.01 ether;

        if (freeMints > 0) {
            if (freeMints < amount) {
                cost = cost - freeMints * 0.01 ether;
                freeMints = 0;
            } else {
                cost = 0;
                freeMints -= amount;
            }
        }

        require(value >= cost, "not enough eth");
        value = value - cost;

        for (uint256 i = 0; i < amount; i++) {
            _mint(receiver, nextTokenId); 
            nextTokenId++;
        }

        // gift the sender back the extra value
        payable(msg.sender).transfer(value);                
    }

    function totalSupply() external view returns (uint256) {
        return nextTokenId;
    }

    function withdraw(address receiver, uint256 amount) external onlyOwner {
        payable(receiver).transfer(amount);
    }
}