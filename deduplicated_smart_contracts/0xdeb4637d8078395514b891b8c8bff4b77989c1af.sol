contract WOAALIAS{
    
    struct Profile{
        uint tokenId;
        string name;
        string description;
    }
    
    mapping(uint=>Profile) private _profileData;
    
    
    function setProfileData(uint _tokenid,string memory _name,string memory _description) public 
    {
        IERC721 tokendata=IERC721(0xEF9c21e3Ba31a74910fc7e7cB3fc814AD842AD6e);
        require(msg.sender==tokendata.ownerOf(_tokenid),"user not a owner of this token id");
        
        _profileData[_tokenid]=Profile({
           tokenId:_tokenid,
           name:_name,
           description:_description
        });
        
    }
    function getProfileData(uint _tokenid)public view returns(string memory name,string memory description){
        Profile storage pfData=_profileData[_tokenid];
        return(pfData.name,pfData.description);
    }
}