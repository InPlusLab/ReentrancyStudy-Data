	function _buildDoge(uint256 _tokenId) private {
        TokenInfo storage _tokenInfo = tokenInfo[_tokenId];
		_tokenInfo.character = Character.Doge;
		string memory tag = 'Doge';
        for (uint256 i; i<dogeAssetParts.length; i++) {
			uint256 randInt = uint256(keccak256(abi.encodePacked(msg.sender, toString(block.timestamp), toString(_tokenId), toString(i))));
			uint256 secret = randInt % 100;
			string memory assetPart = dogeAssetParts[i];
            string memory assetType;
            uint256 assetCount;
			if (dogeAssets[assetPart]['A'].ratio > secret) {
				assetType = 'A';
			} else if (dogeAssets[assetPart]['A'].ratio + dogeAssets[assetPart]['B'].ratio > secret) {
				assetType = 'B';
			} else {
				assetType = 'C';
			}
			if (dogeAssets[assetPart][assetType].assets.length == 0) {
				continue;
			}
			assetCount = dogeAssets[assetPart][assetType].assets.length;
			if (assetCount == 0) {
				continue;
			}
			uint256 randCount = uint256(keccak256(abi.encodePacked(toString(randInt), toString(assetCount))));
            uint256 assetIndex = randCount % assetCount;
			_tokenInfo.attrs[assetPart] = dogeAssets[assetPart][assetType].assets[assetIndex];
			tag = string(abi.encodePacked(tag, '-', assetPart, assetType, toString(assetIndex + 1)));
		}
		_tokenInfo.tag = tag;
	}

	function _buildZombie(uint256 _tokenId) private {
		TokenInfo storage _tokenInfo = tokenInfo[_tokenId];
		_tokenInfo.character = Character.Zombie;
		string memory tag = 'Zombie';
		for (uint256 i; i<zombieAssetParts.length; i++) {
			uint256 randInt = uint256(keccak256(abi.encodePacked(msg.sender, block.timestamp, _tokenId, i)));
			string memory assetPart = zombieAssetParts[i];
			uint256 assetCount = zombieAssets[assetPart].length;
			if (assetCount == 0) {
				continue;
			}
			uint256 randCount = uint256(keccak256(abi.encodePacked(toString(randInt), toString(assetCount))));
			uint256 assetIndex = randCount % assetCount;
			tag = string(abi.encodePacked(tag, '-', assetPart, 'A', toString(assetIndex + 1)));
            _tokenInfo.attrs[assetPart] = zombieAssets[assetPart][assetIndex];
		}
		_tokenInfo.tag = tag;
	}

