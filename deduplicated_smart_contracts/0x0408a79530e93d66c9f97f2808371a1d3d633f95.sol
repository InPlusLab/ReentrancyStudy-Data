// Genetic & Genome intakes one token and allows the user to farm another token.

contract PR_EmiSwap is Ownable {
    using SafeERC20 for IERC20;
    using SafeMath for uint256;

    // Info of each user.
    struct UserInfo {
        uint256 amount; // How many tokens the user has provided.
        uint256 rewardDebt; // Reward debt. See explanation below.
        uint256 remainingEswTokenReward;  // ESW Tokens that weren't distributed for user per pool.
        //
        // Any point in time, the amount of ESW entitled to a user but is pending to be distributed is:
        // pending reward = (user.amount * pool.accESWPerShare) - user.rewardDebt
        //
        // Whenever a user deposits or withdraws Staked tokens to a pool. Here's what happens:
        //   1. The pool's `accESWPerShare` (and `lastRewardTime`) gets updated.
        //   2. User receives the pending reward sent to his/her address.
        //   3. User's `amount` gets updated.
        //   4. User's `rewardDebt` gets updated.
    }
    // Info of each pool.
    struct PoolInfo {
        IERC20 stakingToken; // Contract address of staked token
        uint256 stakingTokenTotalAmount; //Total amount of deposited tokens
        uint256 accEswPerShare; // Accumulated ESW per share, times 1e12. See below.
        uint256 lastRewardTime; // Last timestamp number that ESW distribution occurs.
        uint256 allocPoint; // How many allocation points assigned to this pool. ESW to distribute per second.
    }
    
    IERC20 immutable public esw; // The ESW token
    
    uint256 public eswPerSecond; // ESW tokens vested per second.
    
    PoolInfo[] public poolInfo; // Info of each pool.
    
    mapping(uint256 => mapping(address => UserInfo)) public userInfo; // Info of each user that stakes tokens.
    
    uint256 public totalAllocPoint = 0; // Total allocation points. Must be the sum of all allocation points in all pools.
    
    uint256 immutable public startTime; // The timestamp when ESW farming starts.
    
    uint256 public endTime; // Time on which the reward calculation should end

    event Deposit(address indexed user, uint256 indexed pid, uint256 amount);
    event Withdraw(address indexed user, uint256 indexed pid, uint256 amount);
    event EmergencyWithdraw(address indexed user, uint256 indexed pid, uint256 amount);

    constructor(
        IERC20 _esw,
        uint256 _eswPerSecond,
        uint256 _startTime
    ) {
        esw = _esw;

        eswPerSecond = _eswPerSecond;
        startTime = _startTime;
        endTime = _startTime + 7 days;
    }
    
    function changeEndTime(uint256 addSeconds) external onlyOwner {
        endTime = endTime.add(addSeconds);
    }
    
    // Changes ESW token reward per second. Use this function to moderate the `lockup amount`.
    // Essentially this function changes the amount of the reward which is entitled to the user
    // for his token staking by the time the `endTime` is passed.
    // Good practice to update pools without messing up the contract
    function setEswPerSecond(uint256 _eswPerSecond,  bool _withUpdate) external onlyOwner {
        if (_withUpdate) {
            massUpdatePools();
        }
        eswPerSecond = _eswPerSecond;
    }

    // How many pools are in the contract
    function poolLength() external view returns (uint256) {
        return poolInfo.length;
    }

    // Add a new staking token to the pool. Can only be called by the owner.
    // VERY IMPORTANT NOTICE 
    // ----------- DO NOT add the same staking token more than once. Rewards will be messed up if you do. -------------
    // Good practice to update pools without messing up the contract
    function add(
        uint256 _allocPoint,
        IERC20 _stakingToken,
        bool _withUpdate
    ) external onlyOwner {
        if (_withUpdate) {
            massUpdatePools();
        }
        uint256 lastRewardTime =
            block.timestamp > startTime ? block.timestamp : startTime;
        totalAllocPoint = totalAllocPoint.add(_allocPoint);
        poolInfo.push(
            PoolInfo({
                stakingToken: _stakingToken,
                stakingTokenTotalAmount: 0,
                allocPoint: _allocPoint,
                lastRewardTime: lastRewardTime,
                accEswPerShare: 0
            })
        );
    }

    // Update the given pool's ESW allocation point. Can only be called by the owner.
    // Good practice to update pools without messing up the contract
    function set(
        uint256 _pid,
        uint256 _allocPoint,
        bool _withUpdate
    ) external onlyOwner {
        if (_withUpdate) {
            massUpdatePools();
        }
        totalAllocPoint = totalAllocPoint.sub(poolInfo[_pid].allocPoint).add(_allocPoint);
        poolInfo[_pid].allocPoint = _allocPoint;
    }

    // Return reward multiplier over the given _from to _to time.
    function getMultiplier(uint256 _from, uint256 _to)
        public
        view
        returns (uint256)
    {
        _from = _from > startTime ? _from : startTime;
        if (_from > endTime || _to < startTime) {
            return 0;
        }
        if (_to > endTime) {
            return endTime.sub(_from);
        }
        return _to.sub(_from);
    }

    // View function to see pending ESW on frontend.
    function pendingEsw(uint256 _pid, address _user)
        external
        view
        returns (uint256)
    {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][_user];
        uint256 accEswPerShare = pool.accEswPerShare;
       
        if (block.timestamp > pool.lastRewardTime && pool.stakingTokenTotalAmount != 0) {
            uint256 multiplier =
                getMultiplier(pool.lastRewardTime, block.timestamp);
            uint256 eswReward =
                multiplier.mul(eswPerSecond).mul(pool.allocPoint).div(totalAllocPoint);
            accEswPerShare = eswReward.mul(1e12).div(pool.stakingTokenTotalAmount).add(accEswPerShare);
        }
        return user.amount.mul(accEswPerShare).div(1e12).sub(user.rewardDebt).add(user.remainingEswTokenReward);
    }

    // Update reward variables for all pools. Be careful of gas spending!
    function massUpdatePools() public {
        uint256 length = poolInfo.length;
        for (uint256 pid = 0; pid < length; ++pid) {
            updatePool(pid);
        }
    }

    // Update reward variables of the given pool to be up-to-date.
    function updatePool(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        require(
            block.timestamp > pool.lastRewardTime,
            "Genetic & Genome: block timestamp is less than last reward time"
        );

        if (pool.stakingTokenTotalAmount != 0) {
            uint256 multiplier = getMultiplier(pool.lastRewardTime, block.timestamp);
            uint256 eswReward =
                multiplier.mul(eswPerSecond).mul(pool.allocPoint).div(totalAllocPoint);
            pool.accEswPerShare = 
                eswReward.mul(1e12).div(pool.stakingTokenTotalAmount).add(pool.accEswPerShare);
        }
        pool.lastRewardTime = block.timestamp;
    }

    // Deposit staking tokens to Genetic & Genome for ESW allocation.
    function deposit(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        updatePool(_pid);
        if (user.amount > 0) {
            uint256 pending =
                user.amount.mul(pool.accEswPerShare).div(1e12).sub(user.rewardDebt).add(user.remainingEswTokenReward);
            user.remainingEswTokenReward = safeRewardTransfer(msg.sender, pending);
        }
        pool.stakingToken.safeTransferFrom(
            address(msg.sender),
            address(this),
            _amount
        );
        user.amount = _amount.add(user.amount);
        pool.stakingTokenTotalAmount = _amount.add(pool.stakingTokenTotalAmount);
        user.rewardDebt = user.amount.mul(pool.accEswPerShare).div(1e12);
        emit Deposit(msg.sender, _pid, _amount);
    }

    // Withdraw staked tokens from Genetic & Genome.
    function withdraw(uint256 _pid, uint256 _amount) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        require(
            user.amount >= _amount,
            "Genetic & Genome: you do not have enough tokens to complete this operation"
        );
        updatePool(_pid);
        uint256 pending =
            user.amount.mul(pool.accEswPerShare).div(1e12).sub(user.rewardDebt).add(user.remainingEswTokenReward);
        pool.stakingToken.safeTransfer(address(msg.sender), _amount);
        user.remainingEswTokenReward = safeRewardTransfer(msg.sender, pending);
        user.amount = user.amount.sub(_amount);
        require(
            pool.stakingTokenTotalAmount >= _amount,
            "Genetic & Genome: you do not have enough staking tokens to complete this operation"
        );
        pool.stakingTokenTotalAmount = pool.stakingTokenTotalAmount.sub(_amount);
        user.rewardDebt = user.amount.mul(pool.accEswPerShare).div(1e12);
        emit Withdraw(msg.sender, _pid, _amount);
    }
    
    // Withdraw without caring about rewards. EMERGENCY ONLY.
    function emergencyWithdraw(uint256 _pid) public {
        PoolInfo storage pool = poolInfo[_pid];
        UserInfo storage user = userInfo[_pid][msg.sender];
        uint256 userAmount = user.amount;
        require(
            pool.stakingTokenTotalAmount >= userAmount,
            "Genetic & Genome: you do not have enough staking tokens to complete this operation"
        );
        pool.stakingToken.safeTransfer(address(msg.sender), userAmount);
        pool.stakingTokenTotalAmount = pool.stakingTokenTotalAmount.sub(userAmount);
        user.amount = 0;
        user.rewardDebt = 0;
        user.remainingEswTokenReward = 0;
        emit EmergencyWithdraw(msg.sender, _pid, userAmount);
    }

    // Safe ESW transfer function. Just in case if the pool does not have enough ESW token,
    // The function returns the amount which is owed to the user
    function safeRewardTransfer(address _to, uint256 _amount) internal returns(uint256) {
        uint256 eswTokenBalance = esw.balanceOf(address(this));
        if (_amount > eswTokenBalance) {
            esw.safeTransfer(_to, eswTokenBalance);
            return _amount.sub(eswTokenBalance);
        }
        esw.safeTransfer(_to, _amount);
        return 0;
    }
}