        __Pausable_init_unchained();
        __ERC1155WithMetadata_init(uri);

        _addOperator(minter);

        if (bytes(contractURI_).length != 0) {
            setContractURI(contractURI_);
        }

        if (proxyRegistryAddress_ != address(0)) {
            proxyRegistryAddress = proxyRegistryAddress_;
        }
    }

