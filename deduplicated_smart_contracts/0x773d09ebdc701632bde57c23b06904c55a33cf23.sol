                godId = _to;
            }

            power = power / 2;
            
            if(_to == dogtags[_to].boss) break;
        }
    }
    
    function _comparePower(uint256 power) private view returns(bool) {
        return power >= dogtags[godId].power;
    }
    
     function uint2str(uint256 _i) internal pure returns (string memory _uintAsString) {
		if (_i == 0) {
			return "0";
		}
		uint256 j = _i;
		uint256 len;
		while (j != 0) {
			len++;
			j /= 10;
		}
		bytes memory bstr = new bytes(len);
		uint256 k = len;
		while (_i != 0) {
			k = k - 1;
			uint8 temp = (48 + uint8(_i - (_i / 10) * 10));
			bytes1 b1 = bytes1(temp);
			bstr[k] = b1;
			_i /= 10;
		}
		return string(bstr);
	}
    
    function tokenURI(uint256 tokenId) override public view returns (string memory) {
        
        require(
            _exists(tokenId),
            "token is not exist."
            );
        
        string[13] memory parts;
        
        parts[0] = '<svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" viewBox="0 0 350 350"><style>.base { fill: white; font-family: serif; font-size: 32px; }</style><rect width="100%" height="100%" fill="black" />';
        parts[1] = '<image x="0" y="0" width="350" height="350" xlink:href="';
        parts[2] = dogtags[tokenId].imgUrl;
        parts[3] = '" />';
        parts[4] = '<text x="165" y="300" class="base" text-anchor="middle">';
        parts[5] = getRank(tokenId);
        parts[6] = '</text><text x="165" y="340" class="base" text-anchor="middle">';
        parts[7] = uint2str(dogtags[tokenId].power);
        parts[8] = ' POWER</text>';

        string memory output = string(abi.encodePacked(parts[0], parts[1], parts[2], parts[3], parts[4], parts[5], parts[6], parts[7], parts[8]));
        
        if(dogtags[tokenId].rank > 1)
        {
            parts[9] = '<image x="280" y="0" width="70" height="70" xlink:href="';
            parts[10] = dogtags[dogtags[tokenId].boss].imgUrl;
            parts[11] = '" />';
            
            output = string(abi.encodePacked(output, parts[9], parts[10], parts[11]));
        }
        
        parts[12] = '</svg>';        
        output = string(abi.encodePacked(output, parts[12]));
        
        string memory json = Base64.encode(
            bytes(string(abi.encodePacked(
                '{"name": "#',
                uint2str(tokenId),
                '"',
                ',"boss": "#',
                uint2str(dogtags[tokenId].boss),
                '"',
                ',"rank": "',
                getRank(tokenId),
                '"',
                ',"power": "',
                uint2str(dogtags[tokenId].power),
                '"',
                ',"image": "data:image/svg+xml;base64,',
                Base64.encode(bytes(output)),
                '"}')))
                );
        output = string(abi.encodePacked('data:application/json;base64,', json));

        return output;
    }
    
